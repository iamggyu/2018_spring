M.local_ubion_assign={};M.local_ubion_assign.best=function(Y,cmid){$("#page-mod-assign-view .submissionlinks .btn-assign-best").click(function(){window.open(M.cfg.wwwroot+'/local/ubion/mod/assign/best.php?id='+cmid,'assign_best','width=800, height=600, scrollbars=1')})};M.local_ubion_assign.best_view=function(Y,cmid){$(".path-mod-assign .btn-best-view").click(function(){window.open(M.cfg.wwwroot+'/local/ubion/mod/assign/best_view.php?id='+cmid,'assign_best','width=800, height=600, scrollbars=1')})};M.local_ubion_assign.best_form=function(Y){$(".local_ubion_mod_assign .form-validate").validate({submitHandler:function(form){if($(".local_ubion_mod_assign .form-validate input[name='submissions[]']:checked").length>0){form.submit()}else{alert(M.util.get_string('assign_best_noselected','local_ubion'));return false}}})};M.local_ubion_assign.best_view_popup=function(Y){$(".local_ubion_mod_assign .btn-close").click(function(){window.close()})}